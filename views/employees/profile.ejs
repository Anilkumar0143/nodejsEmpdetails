  <%- include('../includes/head.ejs') %>
  <style>
      .card-container {
          background-color: #231E39;
          border-radius: 5px;
          box-shadow: 0px 10px 20px -10px rgba(0, 0, 0, 0.75);
          color: #B3B8CD;
          padding-top: 30px;
          display: inline;
          width: 350px;
          max-width: 100%;
          text-align: center;
      }



      .card-container .round {
          border: 1px solid #03BFCB;
          border-radius: 50%;
          padding: 7px;
      }



      button.ghost {
          background-color: transparent;

      }

      .skills {
          background-color: #1F1A36;
          text-align: left;
          padding: 25px;
          margin-top: 30px;
      }

      .skills ul {
          list-style-type: none;
          margin: 0;
          padding: 0;
      }

      .skills ul li {
          border: 1px solid #2D2747;
          border-radius: 2px;
          display: inline-block;
          font-size: 12px;
          margin: 2px;
          padding: 8px;
      }


      #body-row {
          background-color: #28223F;
          font-family: Montserrat, sans-serif;
          padding: 6% 2% 0% 0%;
          margin: 1px;
          align-items: center;
          justify-content: center;
          flex-direction: column;
      }
  </style>
  </head>

  <body>
      <%- include('../includes/navlogout.ejs') %>
      <div class="row m-0">
          <!-- Sidebar -->
          <%- include('../includes/sidebar.ejs') %>
          <!-- MAIN -->
          <div class="card-container" id="body-row">

              <img class="round"
                  src="https://scontent.fblr2-1.fna.fbcdn.net/v/t1.0-9/90595729_1088235798202704_4309944104659189760_o.jpg?_nc_cat=110&ccb=3&_nc_sid=09cbfe&_nc_ohc=f3m9kn7Xf7AAX9Zkhz0&_nc_ht=scontent.fblr2-1.fna&oh=9e78501fc1c53a8af4e398ebb2a58b90&oe=605BCA79"
                  width="150px" height="150px" alt="user" />
              <h3 class="m-2"><%= user.firstName %></h3>
              <h6>Indian</h6>
              <p>User interface designer and <br /> front-end developer</p>
              <div class="buttons">
                  <button class="btn btn-danger px-4 py-2">
                      Message
                  </button>
                  <button class="btn btn-primary p-2 px-4 ghost ">
                      Contact
                  </button>
              </div>
              <div class="skills">
                  <h6>Skills</h6>
                  <ul>
                      <li>UI / UX</li>
                      <li>Front End Development</li>
                      <li>HTML</li>
                      <li>CSS</li>
                      <li>JavaScript</li>
                      <li>Ecpress</li>
                      <li>Node</li>
                  </ul>
              </div>
          </div>
          <!-- content -->
          <div class="col " style="padding: 0%; margin:6%  1.6%;">

              <div class="row row-cols-1 row-cols-md-3 g-4">
                  <div class="col">
                      <div class="card h-100">
                          <div class="card-header">
                              <h1><%= user.firstName %> Profile</h1>
                          </div>
                          <div class="card-body ">
                              <h5 class="card-title"> <%= user.firstName %>
                                  <%= user.LastName %></h5>
                              <div class="card-text col">
                                  <div class="row">
                                      <div class="col">Email </div>
                                      <div class="col"> <%= user.useremail %></div>
                                  </div>
                                  <div class="row">
                                      <div class="col"> Contact No </div>
                                      <div class="col"> <%= user.userPhoneNumber %></div>
                                  </div>
                                  <div class="row">
                                      <div class="col"> Gender </div>
                                      <div class="col"><%= user.gender %></div>
                                  </div>
                                  <div class="row">
                                      <div class="col">Date Of Birth </div>
                                      <div class="col"><%= user.dfb %></div>
                                  </div>
                                  <div class="row">
                                      <div class="col"> Ip_Address </div>
                                      <div class="col"><%= user.ip_address %></div>
                                  </div>
                              </div>
                          </div>
                          <div class="card-footer">
                              <div class="row mt-3 mainbtn">
                                  <a href="/selected/<%= user.Id %>" class="mr-2" title="select">
                                      <button class="btn btn-primary p-2">
                                          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                              fill="currentColor" class="bi bi-check2-circle" viewBox="0 0 16 16">
                                              <path
                                                  d="M2.5 8a5.5 5.5 0 0 1 8.25-4.764.5.5 0 0 0 .5-.866A6.5 6.5 0 1 0 14.5 8a.5.5 0 0 0-1 0 5.5 5.5 0 1 1-11 0z" />
                                              <path
                                                  d="M15.354 3.354a.5.5 0 0 0-.708-.708L8 9.293 5.354 6.646a.5.5 0 1 0-.708.708l3 3a.5.5 0 0 0 .708 0l7-7z" />
                                          </svg>
                                      </button>
                                  </a>
                                  <a href="/rejected/<%= user.Id %>" class="mr-2" title="Delete">
                                      <button class="btn btn-primary p-2">
                                          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                              fill="currentColor" class="bi bi-trash" viewBox="0 0 16 16">
                                              <path
                                                  d="M5.5 5.5A.5.5 0 0 1 6 6v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm2.5 0a.5.5 0 0 1 .5.5v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm3 .5a.5.5 0 0 0-1 0v6a.5.5 0 0 0 1 0V6z" />
                                              <path fill-rule="evenodd"
                                                  d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4h-.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1v1zM4.118 4L4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4H4.118zM2.5 3V2h11v1h-11z" />
                                          </svg>
                                      </button>
                                  </a>
                                  <a href="/edit/<%= user.Id %>" class="mr-2" title="Edit">
                                      <button class="btn btn-primary p-2 ">
                                          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                              fill="currentColor" class="bi bi-pencil-square" viewBox="0 0 16 16">
                                              <path
                                                  d="M15.502 1.94a.5.5 0 0 1 0 .706L14.459 3.69l-2-2L13.502.646a.5.5 0 0 1 .707 0l1.293 1.293zm-1.75 2.456l-2-2L4.939 9.21a.5.5 0 0 0-.121.196l-.805 2.414a.25.25 0 0 0 .316.316l2.414-.805a.5.5 0 0 0 .196-.12l6.813-6.814z" />
                                              <path fill-rule="evenodd"
                                                  d="M1 13.5A1.5 1.5 0 0 0 2.5 15h11a1.5 1.5 0 0 0 1.5-1.5v-6a.5.5 0 0 0-1 0v6a.5.5 0 0 1-.5.5h-11a.5.5 0 0 1-.5-.5v-11a.5.5 0 0 1 .5-.5H9a.5.5 0 0 0 0-1H2.5A1.5 1.5 0 0 0 1 2.5v11z" />
                                          </svg>
                                      </button>
                                  </a>
                              </div>
                          </div>
                      </div>
                  </div>
              </div>

          </div>
      </div>
      <%- include('../includes/end.ejs') %>